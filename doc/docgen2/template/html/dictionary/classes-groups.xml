<?xml version="1.0" encoding="windows-1252"?>
<!-- file defines grouping of the classes by their types inside the group

     each group is defined using class-group tag.
     The tag has two attributes:
     title     - the title to be used in the table header and in the content
     template  - is used in case the specific class member body template must be used
     The class-group tag contains the tags type which specifies the class types inside the group.
  -->
<classes-groups>
    <class-group title="Classes" template="write-member-body.xsl" >
        <type name="ref class" display="class" />
        <type name="class" />
        <type name="interface" />
        <type name="struct" />
    </class-group>
    <class-group title="Enumerations" template="write-member-body.xsl" >
        <type name="enum" />
    </class-group>
    <class-group title="Lua Tables" template="write-member-body.xsl" >
        <type name="lua_table" display="table"  />
    </class-group>
    <class-group title="Report Engine Libraries" template="write-relib-body.xsl" >
        <type name="re-library" />
    </class-group>
    <class-group title="Report Engine Statements" template="write-restmt-body.xsl" >
        <type name="re-statements" />
    </class-group>
    <class-group title="Xml Schemas" template="write-member-xml-schema.xsl" >
        <type name="xml-schema" />
    </class-group>
    <class-group title="SQL Tables" template="write-member-body-sql.xsl" >
        <type name="sql" display="table" />
    </class-group>
</classes-groups>
